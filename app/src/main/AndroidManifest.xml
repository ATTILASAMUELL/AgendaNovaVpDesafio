    package="attila.samuell.agendanovavp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AgendaNovaVp">
        <activity android:name=".view.Login"/>
        <activity android:name=".view.Pesquisar" />
        <activity android:name=".view.Cadastro" />
        <activity android:name=".view.MainActivity" />
        <activity android:name=".view.TelaSplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>